<workbook>
	<worksheet name="Users_Upload">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="result" var="user"
			varType="com.assessment.data.User">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0">user.firstName</mapping>
				<mapping row="1" col="1">user.lastName</mapping>
				
				<mapping row="1" col="2">user.email</mapping>
				<mapping row="1" col="3">user.mobileNumber</mapping>
			
				
				<mapping row="1" col="4">user.grade</mapping>
				<mapping row="1" col="5">user.collegeName</mapping>
				<mapping row="1" col="6">user.classifier</mapping>

			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0">End Rows:</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>